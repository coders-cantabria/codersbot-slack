{
  "prod": {
    "app_function": "app.app",
    "aws_region": "eu-west-1",
    "exclude": [
      "__pycache__",
      ".git/*",
      ".gitignore",
      ".python-version",
      "LICENSE",
      "README.md",
      "requirements.txt",
      "zappa_settings.json"
    ],
    "keep_warm": true,
    "keep_warm_expression": "rate(5 minutes)",
    "memory_size": 128,
    "profile_name": "default",
    "project_name": "codersbot-slack",
    "runtime": "python3.6",
    "s3_bucket": "codersbot-slack",
    "timeout_seconds": 30
  }
}
